<?xml version="1.0" encoding="utf-8"?>
<odoo>

<!--        Get stat_users from 360learning-->
        <record id="ir_cron_get_stat_users" model="ir.cron">
            <field name="name">Géneration des statistiques</field>
            <field name="model_id" ref="model_res_partner"/>
            <field name="active" eval="True"></field>
            <field name="state">code</field>
            <field name="code">model.getusers()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="priority" eval="7"/>
        </record>
<!--        supprimer i-One de 360learning -->
       <record id="ir_cron_delete_users" model="ir.cron">
            <field name="name">Suppression automatique d'i-Ones</field>
            <field name="model_id" ref="model_res_partner"/>
             <field name="active" eval="False"></field>

            <field name="state">code</field>
            <field name="code">model.supprimer_ione_auto()</field>
            <field name="interval_number">24</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
           <field name="priority" eval="4" />
        </record>

  <!--        supprimer groupe de session de 360learning -->
       <record id="ir_cron_delete_session" model="ir.cron">
            <field name="name">Suppression automatique des sessions</field>
            <field name="model_id" ref="model_mcmacademy_session"/>
             <field name="active" eval="False"></field>

            <field name="state">code</field>
            <field name="code">model.supprimer_session_automatque()</field>
            <field name="interval_number">24</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
           <field name="priority" eval="4" />


        </record>

       <!-- Ajouter i-One sur 360learning après 14jours si Délai de rétractation n'est pas coché-->
       <record id="ir_cron_delete_groups" model="ir.cron">
            <field name="name">Ajouter i-One sur 360learning  </field>
            <field name="model_id" ref="model_res_partner"/>
             <field name="active" eval="True"></field>
            <field name="state">code</field>
            <field name="code">model.Ajouter_iOne_auto()</field>
            <field name="interval_number">15</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
           <field name="priority" eval="0" />
        </record>
        <!--Changer etat de dossier sur edof avec api  !-->
     <record id="ir_cron_wedof_api" model="ir.cron">
     <field name="name">Mettre en formation les dossiers cpf </field>
     <field name="model_id" ref="model_res_partner"/>
     <field name="active" eval="True"></field>
     <field name="state">code</field>
     <field name="code">model.wedof_api_integration()</field>
     <field name="interval_number">1</field>
     <field name="interval_type">days</field>
     <field name="numbercall">-1</field>
     <field name="priority" eval="3" />
     </record>
       <!--Valider etat de dossier sur edof avec api !-->
     <record id="ir_cron_wedof_api" model="ir.cron">
     <field name="name">Valider les dossiers cpf</field>
     <field name="model_id" ref="model_res_partner"/>
     <field name="active" eval="True"></field>
     <field name="state">code</field>
     <field name="code">model.change_state_wedof_validate()</field>
     <field name="interval_number">15</field>
     <field name="interval_type">minutes</field>
     <field name="numbercall">-1</field>
     <field name="priority" eval="3" />
     </record>



</odoo>