<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="res_config_view_idenfy_form" model="ir.ui.view">
        <field name="name">res.config.view.idenfy.form</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="website.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div[@id='website_settings']" position="after">
                <h2>Idenfy</h2>
                <div class="row mt16 o_settings_container" id="idenfy">
                    <div class="col-12 col-lg-6 o_setting_box" id="idenfy_details">
                        <div class="o_setting_right_pane">
                            <div class="row">
                                <label class="col-lg-3 o_light_label" string="Api Key" for="idenfy_api_key"/>
                                <field name="idenfy_api_key" class="oe_inline"/>
                            </div>
                            <div class="row">
                                <label class="col-lg-3 o_light_label" string="Secret Key" for="idenfy_secret_key"/>
                                <field name="idenfy_secret_key" class="oe_inline"/>
                            </div>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
    </data>
</odoo>