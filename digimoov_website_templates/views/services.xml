<odoo>
    <template id="digimoov_website_templates.digimoov_template_service_clientele" name="Service Clientèle | Digimoov">
        <t t-call="website.layout">
            <div id="wrap" class="oe_structure oe_empty">
                <section class="s_contact_with_img" data-name="Contact Us Two" style="">
                    <div class="contact3 mini-spacer">
                        <div class="container">
                            <div class="row">
                                <div class="row m-0">
                                    <div class="col-lg-6">
                                        <div class="card-shadow ">
                                            <img src="/digimoov_website_templates/static/img/Service client Digimoov.webp"
                                                 class="img-responsive img img-fluid"
                                                 data-original-title="" title="" aria-describedby="tooltip508208"/>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="contact-box ">
                                            <span style="font-size: 28px;">Contacter le service client</span>
                                            <br/>
                                            <form action="/helpdesk/submitted/" method="post"
                                                  enctype="multipart/form-data">
                                                <input type="hidden" name="service" value="client"/>
                                                <div class="row">
                                                    <div class="col-lg-6">
                                                        <div class="form-group m-t-15 ">
                                                            <input placeholder="Nom" name="contact_lastname"
                                                                   required="True" type="text"
                                                                   class="o_website_form_input   font-14 form-control"
                                                                   t-attf-value="#{contact_last_name}"/>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6">
                                                        <div class="form-group m-t-15 ">
                                                            <input placeholder="Prénom" name="contact_name"
                                                                   required="True" type="text"
                                                                   class="o_website_form_input   font-14 form-control"
                                                                    t-attf-value="#{contact_name}"/>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-12">
                                                        <div class="form-group m-t-15 ">
                                                            <input name="email_from" placeholder="Adresse email"
                                                                   required="True" value="" type="email"
                                                                   class="o_website_form_input form-control" t-attf-value="#{email_from}"/>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-12">
                                                        <div class="form-group m-t-15 ">
                                                            <input name="phone" placeholder="Téléphone" required="True"
                                                                   type="Phone"
                                                                   class="o_website_form_input form-control" t-attf-value="#{phone}"/>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-12">
                                                        <div class="form-group m-t-15 ">
                                                            <input name="name" placeholder="Objet" required="True"
                                                                   type="Object"
                                                                   class="o_website_form_input form-control" value=""/>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-12">
                                                        <div class="form-group m-t-15 ">
                                                            <textarea name="description" rows="3" placeholder="Message"
                                                                      required="True"
                                                                      class=" o_website_form_input form-control"
                                                                      value=""/>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-12 s_name" data-name="Button">
                                                        <button class="o_website_form_send btn btn-alpha rounded-circle m-t-20 arrow-btn"
                                                                type="submit">
                                                            ENVOYER
                                                            <span class="fa fa-1x fa-fw fa-long-arrow-right ml-1"/>
                                                        </button>
                                                        <span id="o_website_form_result"
                                                              class="text-danger ml8 mt16  mb16 col-md-12"/>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
            </div>

        </t>
    </template>
