<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_partner_form_onfido" model="ir.ui.view">
        <field name="name">res.partner.onfido.inherit</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="sendinblue.view_partner_sendinblue_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook[last()]" position="inside">
                <page string="Onfido " name="onfido_info">
                    <field name="onfido_information_ids" >
                      <tree>
                            <field name="workflow_run_id"/>
                            <field name="sdk_token"/>
                            <field name="id_document_back" />
                            <field name="id_document_front" />
                            <field name="validation_onfido"></field>

                        </tree>
                    </field>
                </page>
            </xpath>
        </field>
    </record>
</odoo>