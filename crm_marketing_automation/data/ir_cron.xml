<?xml version="1.0" encoding="utf-8"?>
<odoo>

<!--Changer le statut des apprenants vers non retracté dans crm lead-->
        <record id="ir_cron_change_stage_lead" model="ir.cron">
            <field name="name">Changer statut en crm lead vers non retracté</field>
            <field name="model_id" ref="model_res_partner"/>
            <field name="active" eval="True"></field>
            <field name="state">code</field>
            <field name="code">model.change_stage_non_retracte()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>

   <!--Classer  dans crm lead les apprenants existant déja -->
        <record id="ir_cron_classe_stage_lead" model="ir.cron">
            <field name="name">Classer les apprenants sur crm lead </field>
            <field name="model_id" ref="model_res_partner"/>
            <field name="active" eval="True"></field>
            <field name="state">code</field>
            <field name="code">model.change_stage_existant()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">1</field>
        </record>
</odoo>