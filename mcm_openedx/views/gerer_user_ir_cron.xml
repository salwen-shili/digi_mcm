<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--  tache cron pour executer la fonction de l'affectation des Coaches -->
    <record id="ir_cron_affecter_coach" model="ir.cron">
        <field name="name">Affecter les coach pour des apprenants</field>
        <field name="model_id" ref="model_mcm_openedx_coach"/>
        <field name="active" eval="True"></field>
        <field name="state">code</field>
        <field name="code">model.test_coach()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="priority" eval="1"/>
    </record>
    <record id="ir_cron_affecter_societe" model="ir.cron">
        <field name="name">Tester si un Client bolt
        </field>
        <field name="model_id" ref="model_res_partner"/>
        <field name="active" eval="True"></field>
        <field name="state">code</field>
        <field name="code">model.estBolt()</field>
        <field name="interval_number">59</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="priority" eval="1"/>
    </record>
    <record id="ir_cron_egaliter_coach" model="ir.cron">
        <field name="name">Egalité entre les coachhes</field>
        <field name="model_id" ref="model_mcm_openedx_coach"/>
        <field name="active" eval="False"></field>
        <field name="state">code</field>
        <field name="code">model.egalité()</field>
        <field name="interval_number">59</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="priority" eval="1"/>
    </record>
    <!-- tache cron pour ajouter les apprenants -->
    <record id="ir_cron_ajouter" model="ir.cron">
        <field name="name">ajout automatique des apprennats a moocit
        </field>
        <field name="model_id" ref="model_res_partner"/>
        <field name="active" eval="True"></field>
        <field name="state">code</field>
        <field name="code">model.ajoutMoocit_automatique()</field>
        <field name="interval_number">58</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="priority" eval="3"/>
    </record>
    <!-- tache cron pour voir statut en attent edof -->
    <record id="ir_cron_edof_moocit" model="ir.cron">
        <field name="name">edof verification
        </field>
        <field name="model_id" ref="model_mcm_openedx_enattente"/>
        <field name="active" eval="True"></field>
        <field name="state">code</field>
        <field name="code">model.wedof_api_integration_moocit()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="priority" eval="4"/>
    </record>
    <!-- tache cron pour update_datesupp -->
    <record id="ir_cron_update_datesupp" model="ir.cron">
        <field name="name">update_datesupp
        </field>
        <field name="model_id" ref="model_res_partner"/>
        <field name="active" eval="True"></field>
        <field name="state">code</field>
        <field name="code">model.update_datesupp()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="priority" eval="5"/>
    </record>
    <!-- tache cron pour affecter les coaches selon la premiere personne qui a ecrit un comm -->
    <record id="ir_cron_aff_coach" model="ir.cron">
        <field name="name">affecter les coaches selon la premiere personne qui a ecrit comm
        </field>
        <field name="model_id" ref="model_mcm_openedx_coach"/>
        <field name="active" eval="False"></field>
        <field name="state">code</field>
        <field name="code">model.aff_coach()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="priority" eval="3"/>
    </record>
