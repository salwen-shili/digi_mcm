    <!-- tache cron pour ajouter les apprenants -->
    <data noupdate="1">

    <record id="ir_cron_ajouter" model="ir.cron">
        <field name="name">ajout automatique des apprennats a moocit
        </field>
        <field name="model_id" ref="model_res_partner"/>
        <field name="active" eval="True"></field>
        <field name="state">code</field>
        <field name="code">model.ajoutMoocit_automatique()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">weeks</field>
        <field name="numbercall">-1</field>
        <field name="priority" eval="3"/>
    </record>
    </data>
    <!-- tache cron pour voir statut en attent edof -->
    <data noupdate="1">

    <record id="ir_cron_edof_moocit" model="ir.cron">
        <field name="name">edof verification
        </field>
        <field name="model_id" ref="model_mcm_openedx_enattente"/>
        <field name="active" eval="True"></field>
        <field name="state">code</field>
        <field name="code">model.wedof_api_integration_moocit()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="priority" eval="4"/>
    </record>
    </data>
    <!-- tache cron poursupprimer les apprenant reussit-->
    <data noupdate="1">

    <record id="ir_cron_supp_admis" model="ir.cron">
        <field name="name">Supprimer les apprenant ayant resulta (Admis)
        </field>
        <field name="model_id" ref="model_res_partner"/>
        <field name="active" eval="True"></field>
        <field name="state">code</field>
        <field name="code">model.supp_Réussie()</field>
        <field name="interval_number">30</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="priority" eval="3"/>
    </record>
    </data>
    <!-- tache cron pour supprimer les apprenant repassage exman-->
    <data noupdate="1">

    <record id="ir_cron_rep_exman" model="ir.cron">
        <field name="name">Reppasage exmman Vtc/TAXI
        </field>
        <field name="model_id" ref="model_res_partner"/>
        <field name="active" eval="False"></field>
        <field name="state">code</field>
        <field name="code">model.repasage_exman()</field>
        <field name="interval_number">30</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="priority" eval="3"/>
    </record>

    </data>
    <!-- tache cron pour importer la liste des event sur calendly -->
    <data noupdate="1">

    <record id="ir_crontype_event" model="ir.cron">
        <field name="name">Ajouter Type event
        </field>
        <field name="model_id" ref="model_mcm_openedx_calendly_event"/>
        <field name="active" eval="True"></field>
        <field name="state">code</field>
        <field name="code">model.event()</field>
        <field name="interval_number">6</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="priority" eval="3"/>
    </record>
    </data>
    <data noupdate="1">

    <record id="ir_cron_event" model="ir.cron">
