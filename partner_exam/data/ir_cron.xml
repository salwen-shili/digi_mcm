<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.cron" id="remove_duplicate_exams">
        <field name="name">Examen :Suppression de duplication des examens</field>
        <field name="model_id" ref="model_info_examen"/>
        <field name="state">code</field>
        <field name="code">model._clear_duplicates_exams()</field>
        <field name="active" eval="True"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">30</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
    </record>



    <record model="ir.cron" id="mode_financement">
        <field name="name">Info examen  :Remplir mode de financement</field>
        <field name="model_id" ref="model_info_examen"/>
        <field name="state">code</field>
        <field name="code">model.mise_ajour_mode_financement()</field>
        <field name="active" eval="False"/>
        <field name="interval_number">30</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">1</field>
    </record>
</odoo>