<odoo>
    <!--    This is used to reformat paper format of report PDF Convocation-->
    <record id="paperformat_convocation" model="report.paperformat">
        <field name="name">Convocation</field>
        <field name="default" eval="False"/>
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">10</field>
        <field name="margin_right">10</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">10</field>
        <field name="dpi">90</field>
    </record>

    <!--    This report action create a print pdf for "convocation", using the template(xml) ID in contact view-->
    <report id="id_convocation_print_report_pdf_ids'"
            string="Convocation"
            model="res.partner"
            report_type="qweb-pdf"
            name="partner_exam.id_convocation_print_report_pdf_ids"
            file="partner_exam.id_convocation_print_report_pdf_ids"
            print_report_name= "'Convocation ' + (object.print_report_name_partner())"
            paperformat="partner_exam.paperformat_convocation"
    />

    <!-- This report action create a report in exam view-->
    <report id="convocation_examen_ids'"
            string="Convocation"
            model="info.examen"
            report_type="qweb-pdf"
            name="partner_exam.examen_convocation_id"
            file="partner_exam.convocation_examen_ids"
            print_report_name="'Convocation ' + (object.print_report_name_examen())"
            paperformat="partner_exam.paperformat_convocation"
    />

    <!--    This is used to reformat paper format of report PDF Relevé de Note-->
    <record id="paperformat_releve_note_id" model="report.paperformat">
        <field name="name">Relevé de note</field>
        <field name="default" eval="False"/>
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">15</field>
        <field name="margin_right">15</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">0</field>
        <field name="dpi">90</field>
    </record>

    <!--    This is the action to create a print pdf for "Générer les relevés de notes", using the template(xml) ID-->
    <report id="report_releve_note_ids"
            model="res.partner"
            string="Générer les relevés de notes"
            report_type="qweb-pdf"
            name="partner_exam.id_many_releve_note_print_report_pdf"
            file="partner_exam.report_releve_note_id"
            print_report_name="'Relevé de notes ' + (object.print_report_name_partner())"
            paperformat="partner_exam.paperformat_releve_note_id"/>

    <!-- This is the action to create a print pdf in module info.examen for "Générer les relevés de notes", using the template(xml) ID-->
    <report id="examen_releve_note_ids"
            model="info.examen"
            string="Relevé de note"
            report_type="qweb-pdf"
            name="partner_exam.examen_releve_note_id"
            file="partner_exam.examen_releve_note_ids"
            menu="True"
            print_report_name="'Relevé de notes ' + (object.print_report_name_examen())"
            paperformat="partner_exam.paperformat_releve_note_id"/>

    <!-- Rapport attestation de suivi de formation dans l'interface de notes des examen, le menu existe dans la session-->
    <report id="examen_suivi_de_formation_ids"
            model="info.examen"
            string="Attestation de suivi de formation"
            report_type="qweb-pdf"
            name="partner_exam.examen_attestation_suivi_formation_id"
            file="partner_exam.examen_suivi_de_formation_ids"
            menu="True"
            print_report_name="'Attestation suivi formation ' + (object.print_report_name_examen())"
            paperformat="partner_exam.paperformat_releve_note_id"/>

    <!-- Rapport cerfa dans l'interface de notes des examen, le menu existe dans la session-->
    <report id="examen_cerfa_id"
            model="info.examen"
            string="Cerfa"
            report_type="qweb-pdf"
            name="partner_exam.examen_rapport_cerfa_id"
            file="partner_exam.examen_cerfa_id"
            menu="True"
            print_report_name="'CERFA ' + (object.print_report_name_examen())"
            paperformat="partner_exam.paperformat_cerfa"/>

    <!--This is the action to create a print pdf in module res.partner for "Attestation de suivi de formation", using the template(xml) ID-->
    <report id="id_attestation"
            model="res.partner"
            string="Attestation Suivi Formation"
            report_type="qweb-pdf"
            name="partner_exam.attestation_suivi_formation_id"
            file="partner_exam.id_attestation"
            print_report_name="'Attestation suivi formation ' + (object.print_report_name_partner())"
            paperformat="partner_exam.paperformat_releve_note_id"/>

    <!--    This is used to reformat paper pdf format of cerfa report -->
    <record id="paperformat_cerfa" model="report.paperformat">
        <field name="name">Cerfa</field>
        <field name="default" eval="False"/>
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">10</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">5</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">0</field>
        <field name="dpi">90</field>
    </record>

    <!--    This is used to reformat paper pdf format of connection statement -->
    <record id="paperformat_connection_statement" model="report.paperformat">
        <field name="name">Un relevé de connexion</field>
        <field name="default" eval="False"/>
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">10</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">5</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">0</field>
        <field name="dpi">90</field>
    </record>

    <!--This is used to reformat paper pdf format of RAPPORT DE SESSION D’EXAMEN -->
    <record id="paperformat_rapport_session_examen" model="report.paperformat">
        <field name="name">RAPPORT DE SESSION D’EXAMEN</field>
        <field name="default" eval="False"/>
        <field name="format">A4</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">10</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">15</field>
        <field name="margin_right">15</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">0</field>
        <field name="dpi">90</field>
    </record>

    <!-- Cerfa -->
    <report id="report_cerfa"
            string="Cerfa"
            model="res.partner"
            report_type="qweb-pdf"
            name="partner_exam.template_raport_cerfa"
            file="partner_exam.report_cerfa"
            print_report_name="'CERFA ' + (object.print_report_name_partner())"
            paperformat="partner_exam.paperformat_cerfa"
    />

    <!-- Connection statement in partner view (Générer un relevé de connexion)-->
    <report id="pdf_connection_statement_id"
            string="Générer un relevé de connexion"
            model="res.partner"
            report_type="qweb-pdf"
            name="partner_exam.pdf_connection_statement_report"
            file="partner_exam.pdf_connection_statement_id"
            print_report_name="'Relevé de connexion ' + (object.print_report_name_partner())"
            paperformat="partner_exam.paperformat_connection_statement"
    />
    <!-- Connection statement in exam view (Générer un relevé de connexion)-->
    <report id="exam_connection_statement_id"
            string="Générer un relevé de connexion"
            model="info.examen"
            report_type="qweb-pdf"
            name="partner_exam.examen_connection_statement"
            file="partner_exam.exam_connection_statement_id"
            print_report_name="'Relevé de connexion ' + (object.print_report_name_examen())"
            paperformat="partner_exam.paperformat_connection_statement"
    />

    <!-- RAPPORT DE SESSION D’EXAMEN -->
    <report id="pdf_rapport_session_examen_id"
            string="RAPPORT DE SESSION D’EXAMEN"
            model="res.partner"
            report_type="qweb-pdf"
            name="partner_exam.pdf_rapport_session_examen"
            file="partner_exam.pdf_rapport_session_examen_id"
            print_report_name="'RAPPORT DE SESSION D’EXAMEN ' + (object.print_report_name_partner())"
            paperformat="partner_exam.paperformat_connection_statement"
    />

    <!--  Menu de rapport pdf "Attestation d'entrée en formation" MCM_DIGI -->
    <report id="pdf_rapport_entree_formation_id"
            string="Attestation d'entrée en formation"
            model="res.partner"
            report_type="qweb-pdf"
            name="partner_exam.attestation_entree_formation_report"
            file="partner_exam.pdf_rapport_entree_formation_id"
            print_report_name="'Attestation d’entrée en formation ' + (object.print_report_name_partner())"
            paperformat="partner_exam.paperformat_connection_statement"
    />

</odoo>