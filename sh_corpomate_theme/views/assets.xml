<?xml version="1.0" encoding="utf-8"?>
<odoo>
      <template id="sh_corpomate_theme_assets_wysiwyg" inherit_id="website.assets_wysiwyg" name="Website Editor Assets (used in website editor)">
			<xpath expr="." position="inside">
			<script type="text/javascript" src="/sh_corpomate_theme/static/src/js/editor/widget_link.js"/>
			</xpath>
	   </template>
		<!-- TIMER SNIPPET  -->
	  <template id="sh_corpomate_theme_timer_snippet_assets_editor" inherit_id="website.assets_editor" name="Offer Timer Editor assets">
	    <xpath expr="." position="inside">
	      <script type="text/javascript" src="/sh_corpomate_theme/static/src/js/timer_snippet/timer_editor.js" ></script>       
	    </xpath>
	  </template>
	  


