        <xpath expr="." position="inside">
      
        <link rel="stylesheet" type="text/scss" href="/sh_corpomate_theme/static/src/scss/as_mixin.scss"/>    
	    <link href="https://fonts.googleapis.com/css?family=Titillium+Web&amp;display=swap" rel="stylesheet"/>
        <link href="/sh_corpomate_theme/static/src/scss/owl.carousel.css" rel="stylesheet"></link>
        <link href="/sh_corpomate_theme/static/src/scss/owl.theme.default.min.css" rel="stylesheet"></link>
        <link rel="stylesheet" type="text/scss" href="/sh_corpomate_theme/static/src/scss/variables.scss"/>                    
        <link rel="stylesheet" type="text/scss" href="/sh_corpomate_theme/static/src/scss/style.scss"/>
        <link rel="stylesheet" type="text/scss" href="/sh_corpomate_theme/static/src/scss/blog.scss"/>
        <link rel="stylesheet" type="text/scss" href="/sh_corpomate_theme/static/src/scss/event.scss"/>
        <link rel="stylesheet" type="text/scss" href="/sh_corpomate_theme/static/src/scss/job.scss"/>
        <link rel="stylesheet" type="text/scss" href="/sh_corpomate_theme/static/src/scss/help.scss"/>
        <link rel="stylesheet" type="text/scss" href="/sh_corpomate_theme/static/src/scss/animate.css"></link>
           
           
		<!-- Theme 13 snippets -->          
        <link rel="stylesheet" type="text/scss" href="/sh_corpomate_theme/static/src/scss/theme/theme_13/theme_13_s.scss"/>
        <link rel="stylesheet" type="text/scss" href="/sh_corpomate_theme/static/src/scss/theme/theme_14/theme_14_s.scss"/>
        <link rel="stylesheet" type="text/scss" href="/sh_corpomate_theme/static/src/scss/theme/theme_15/theme_15_s.scss"/>
        <link rel="stylesheet" type="text/scss" href="/sh_corpomate_theme/static/src/scss/theme/theme_16/theme_16_s.scss"/>          
            
        <!-- Mega Menu -->
        <link rel="stylesheet" type="text/scss" href="/sh_corpomate_theme/static/src/scss/mega_menu/sh_website_megamenu.scss"/>
		<script type="text/javascript" src="/sh_corpomate_theme/static/src/js/mega_menu/custom.js"/>
			
			            
                        
           
        <script src="/sh_corpomate_theme/static/src/js/owl.carousel.js" type="text/javascript"></script> 
        <script rel="stylesheet" type="text/javascript" src="/sh_corpomate_theme/static/src/js/index.js"/>
        <script type="text/javascript" src="/sh_corpomate_theme/static/src/js/wow_min.js"></script>
      	<script type="text/javascript" src="/sh_corpomate_theme/static/src/js/render_tess.js"/>
		<script type="text/javascript" src="/sh_corpomate_theme/static/src/js/render_our_partner.js"/>
			
			
        <!-- TIMER SNIPPET -->
        <script type="text/javascript" src="/sh_corpomate_theme/static/src/js/timer_snippet/timer_front.js"></script>  			
			
			
        <!-- TAB SLIDER SNIPPET -->          
        <script type="text/javascript" src="/sh_corpomate_theme/static/src/js/tab_slider_snippet/slider.js"></script>  			
			
	

			
        </xpath>
