                  <div class="row">
                    <div class="col-md-4">
                      <div class="form-group sh_name sh_group form-field o_website_form_required_custom">
                        <input type="text" class="form-control o_website_form_input" placeholder="Name" name="contact_name" required="" value=""/>
                        <div class="sh_border">
                          <div class="sh_left_right_border"/>
                        </div>
                      </div>
                    </div>
                    <div class="col-md-4">
                      <div class="form-group sh_email sh_group form-field">
                        <input type="text" class="form-control o_website_form_input" placeholder="Phone" name="phone" value=""/>
                        <div class="sh_border">
                          <div class="sh_left_right_border"/>
                        </div>
                      </div>
                    </div>
                    <div class="col-md-4">
                      <div class="form-group sh_number sh_group form-field o_website_form_required_custom">
                        <input type="text" class="form-control o_website_form_input" placeholder="Email" name="email_from" required="" value=""/>
