<!-- Vue helpdesk de MCM ACADEMY dans le siteweb -->
<odoo>
    <!-- Vue helpdesk de Digimoov de tous les services dans le siteweb -->
    <template id="mcm_website_theme.mcm_template_contact" name="Contactez-Nous | MCM Academy">

        <t t-call="website.layout">

            <div id="wrap" class="oe_structure oe_empty">
                <section class="centric_template" data-name="Projects">
                    <section class="project-section pt128 pb0">
                        <div class="container">
                            <div class="project-title text-center">
                                <br/>
                                <h3>
                                    Contacter MCM ACADEMY
                                </h3>
                            </div>
                            <section class="s_web_agency_images_with_center_para bg-light-1" data-name="Images with center aragraph" style="">
                                <div class="spacer ">
                                    <div class="container">
                                        <div class="row m-t-40">
                                            <div class="col-md-4">
                                                <div class="card card-shadow aos-init" data-aos="flip-left" data-aos-duration="1200">
                                                    <a href="/service-clientele" class="img-ho">
                                                        <img class="card-img-top lazy" src="/mcm_website_theme/static/src/img/contact/service_clientéle.png" alt="" data-original-title="" title="" aria-describedby="tooltip486395"/>
                                                    </a>
                                                    <div class="card-body">
                                                        <h5 class="font-medium m-b-0">
                                                            <a href="/service-clientele">Service clientèle</a>
                                                        </h5>
                                                        <p class="font-14 m-b-0">Notre service client est à votre
                                                            disposition pour répondre à toutes vos questions.
                                                        </p>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="card card-shadow aos-init" data-aos="flip-up" data-aos-duration="1200">
                                                    <a href="/partenariat" class="img-ho">
                                                        <img class="card-img-top lazy" src="/mcm_website_theme/static/src/img/contact/partenariat.png" alt="" data-original-title="" title="" aria-describedby="tooltip155657"/>
                                                    </a>
                                                    <div class="card-body">
                                                        <h5 class="font-medium m-b-0">
                                                            <a href="/partenariat">Partenariat</a>
                                                        </h5>
                                                        <p class="font-14 m-b-0">Contactez-nous par ici si vous
                                                            souhaitez devenir partenaire avec Mcm Academy.
                                                        </p>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="card card-shadow aos-init" data-aos="flip-up" data-aos-duration="1200">
                                                    <a href="/service-comptabilite" class="img-ho">
                                                        <img class="card-img-top lazy" src="/mcm_website_theme/static/src/img/contact/service comptabilité.png" alt="" data-original-title="" title="" aria-describedby="tooltip4033"/>
                                                    </a>
                                                    <div class="card-body">
                                                        <h5 class="font-medium m-b-0">
                                                            <a href="/service-comptabilite">Service comptabilité</a>
                                                        </h5>
                                                        <p class="font-14 m-b-0">Vous souhaitez suivre vos paiements?
                                                            Demandez-le ici.
                                                        </p>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="card card-shadow aos-init" data-aos="flip-right" data-aos-duration="1200">
                                                    <a href="/service-pedagogique" class="img-ho">
                                                        <img class="card-img-top lazy" src="/mcm_website_theme/static/src/img/contact/service péda.png" alt="" data-original-title="" title="" aria-describedby="tooltip88003"/>
                                                    </a>
                                                    <div class="card-body">
                                                        <h5 class="font-medium m-b-0">
                                                            <a href="/service-pedagogique">Service pédagogique</a>
                                                        </h5>
                                                        <p class="font-14 m-b-0">Vous êtes déjà inscris?
                                                            <span class="fa fa-info-circle" data-original-title="" title="Service réservé aux abonnés" aria-describedby="tooltip551325" style="color: rgb(99, 74, 165);"/>
                                                            Contactez le service pédagogique de Mcm academy par ici.
                                                        </p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div>
                    </section>
                </section>
            </div>
        </t>
    </template>
</odoo>