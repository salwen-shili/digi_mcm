    <!--FORMATION
    TAXI PREMIUM -->
    <template id="mcm_website_theme.pack_taxi_premium">
        <!-- Will be added in side the div commented below-->
        <!-- <div class="col-md-4 sh_animation_part js-scroll fade-in-top"> -->
        <t t-foreach="mcm_products" t-as="product">
            <t t-if="product.default_code=='taxi'">
                <div class="sh_pricing">
                    <form action="/shop/cart/update" method="post" id="form-pack-taxi-premium">
                        <input type="hidden" class="product_id" name="product_id" id="taxi_product"
                            t-att-value="product.id" />
                        <input type="hidden" name="promo" t-att-value="promo.id" t-if="promo" />
                        <h4 style="padding-bottom:0px !important">FORMATION TAXI PREMIUM</h4>
                        <span
                            style="margin-top: 0px;padding: 0px;font-weight: 600;color: #060606 !important;background: #fdd105f7;padding: 0 5px;font-size: 18px;">
                            Frais d'examen inclus
                        </span>
                        <h5 style="margin-top: 20px !important;
    margin-bottom: 0.5rem !important;
    font-size: 18px !important;">
                            <b>
                                <t t-if="product.cpf_available == false">NON </t>ELIGIBLE AU CPF </b>
                        </h5>
                        <h4>
                            <span t-esc="round(product.list_price)" />
                            <span class="prod-cur" t-esc="product.currency_id.symbol" />
                            <!-- <span class="">*</span> -->
                        </h4>
                        <t t-call="mcm_website_theme.pricing_table_content_all" />
                        <br />
                        <t t-if="promo">
                            <p class="text-center font-weight-bolder text-secondary">
                                <b style="font-size: 50%">
                                    <t />
                                </b>
                            </p>
                        </t>
                        <div class="sh_btn">
                            <a class="animated-button-yellow" href="#pricing"
                                onClick="(function(){ document.getElementById('form-pack-taxi-premium').submit();return false;})();return false;">
                                <span />
                                <span />
                                <span />
                                <span /> Je m'inscris </a>
                            <div>
                                <span
                                    style="color: #005a00 !important;font-size: 17px;font-weight: 600;"> Paiement 1 <t t-if="product.instalment_number&gt;1"> ou <t t-esc="product.instalment_number" /></t>
                                    &amp;nbsp;fois </span>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="text-center">
                    <span style="
                    font-weight: 600;
                    font-size: 13px;
                ">* Offre valable pour les paiements par carte bleu</span>
                </div>
            </t>
        </t>
    </template>
