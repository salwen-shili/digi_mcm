        <!--  Import calls planned action ..executed every 1 minute  -->
        <record id="import_call_schedulers" model="ir.cron">
            <field name="name">Import Call records form AirCall</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="model_id" ref="model_res_users" />
            <field name="state">code</field>
            <field name="active">1</field>
            <field name="code">model.import_calls()</field>
            <field name="interval_number">8</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>




