<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="mcm_website_partner_not_found">
        <p class="card-text"
           style="text-align: left; ">Email incorrect ou non trouvé.Client n'a pas été crée
        </p>
    </template>
    <template id="mcm_website_module_not_found">
